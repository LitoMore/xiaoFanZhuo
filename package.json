{
  "name": "xiaoFanZhuo",
  "version": "0.5.9",
  "main": "app.js",
  "dependencies": {
    "electron": "^1.8.4",
    "electron-config": "^0.2.1",
    "oauth": "^0.9.15",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "electron-packager": "^8.7.2"
  },
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/logo.png --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/logo.ico --out=xiaoFanZhuo --version-string.CompanyName=TAT --version-string.FileDescription=xiaoFanZhuo --version-string.ProductName=\"小饭桌\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  }
}
